<script type="text/javascript">
	function showMenu(menu) {
		var menuElement = document.getElementById(menu);
		if (menu == 'findData2') {
			var header = document.getElementById("findDataHeader");
		} else {
			var header = document.getElementById(menu + "Header");
		}
		if (menuElement.style.display == "none") {
			menuElement.style.display = "block";
			header.style.paddingBottom = "10px";
			header.style.height = header.offsetHeight - 20 + "px";
		} else {
			menuElement.style.display = "none";
			header.style.paddingBottom = "20px";
			header.style.height = "auto";
		}
		var panel = header.parentElement.parentElement;
		for (i = 0; panel.className != "nav_panel"; i++) {
			panel = panel.parentElement;
		}
		
		if (menu == 'findData') {
			showMenu('findData2');
		}
	}
	function revealElement(E) {
			ActiveElement = document.getElementById(E);
			
			if (ActiveElement.style.display == "block") {
				ActiveElement.style.display = "none";
			} else {
				ActiveElement.style.display = "block";
			}
		}
</script>
